{
    "clone a project": "git clone <project_url>",
    "for taking pull": "git pull",
    "for adding all files in stazing area": "git add .",
    "for staze only particular file": "git add <file_name>",
    "git command for commit": "git commit -m 'any message'",
    "command for push our code": "git push",
    "for stash": "git stash(for hiding are changes)",
    "undo stash": "git stash apply(for bringing our stash changes)"
}
